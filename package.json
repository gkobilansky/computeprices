    "scrape:all:dry": "npm run scrape:lambda:dry && npm run scrape:vast:dry && npm run scrape:coreweave:dry && npm run scrape:fluidstack:dry && npm run scrape:aws:dry && npm run scrape:runpod:dry && npm run scrape:datacrunch:dry",
    "scrape:datacrunch": "node -r dotenv/config scripts/scrape-datacrunch.js dotenv_config_path=.env.local",
